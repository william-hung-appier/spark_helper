{
  "name": "Appier Spark Helper",
  "description": "Quickly generate SQL snippet for Spark queries",
  "version": "1.1.3",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": "src/assets/icon.png"
  },
  "icons": {
    "16": "src/assets/icon.png",
    "32": "src/assets/icon.png",
    "48": "src/assets/icon.png",
    "128": "src/assets/icon.png"
  },
  "permissions": ["clipboardWrite"],
  "host_permissions": ["https://data-catalog.dp.arepa.appier.info/*"]
}
